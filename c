import java.util.*;
class Main{
	public static void main(String args[])
	{
		Scanner s=new Scanner(System.in);
		int a=s.nextInt();
		int b=s.nextInt();
		int res=sum(a,b);
		
		System.out.println(res);
	}

	private static int sum(int a, int b) {
		// TODO Auto-generated method stub
		return a+b;
	}
}
